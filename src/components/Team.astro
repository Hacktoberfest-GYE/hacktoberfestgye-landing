---
const logos = [
  {
    url: '#',
    image: '/images/logos/kernel.webp',
  },
  {
    url: '#',
    image: '/images/logos/herux.webp',
  },
  {
    url: '#',
    image: '/images/logos/phycom.webp',
  },
  {
    url: '#',
    image: '/images/logos/pythonecuador.webp',
  },
  {
    url: '#',
    image: '/images/logos/aws-ecuador.svg',
  },
  {
    url: '#',
    image: '/images/logos/wordpress_guayaquil.webp',
